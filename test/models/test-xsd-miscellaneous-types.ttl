@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.0.0#> .
@prefix samm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.0.0#> .
@prefix samm-e: <urn:samm:org.eclipse.esmf.samm:entity:2.0.0#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:2.0.0#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <urn:samm:org.eclipse.esmf.test:1.0.0#> .

:TestXSDMiscellaneousTypes a samm:Aspect ;
   samm:properties ( :a :b :c ) ;
   samm:operations ( ) ;
   samm:name "TestXSDMiscellaneousTypes" .

:a a samm:Property ;
   samm:characteristic :AnyUriCharacteristic ;
   samm:name "a" .

:b a samm:Property ;
   samm:characteristic :CurieCharacteristic ;
   samm:name "b" .

:c a samm:Property ;
   samm:characteristic :RDFLangStringCharacteristic ;
   samm:name "c" .

:AnyUriCharacteristic a samm:Characteristic ;
   samm:dataType xsd:anyUri ;
   samm:name "AnyUriCharacteristic" .

:CurieCharacteristic a samm:Characteristic ;
   samm:dataType samm:curie ;
   samm:name "CurieCharacteristic" .

:RDFLangStringCharacteristic a samm:Characteristic ;
   samm:dataType rdf:langString ;
   samm:name "RDFLangStringCharacteristic" .
